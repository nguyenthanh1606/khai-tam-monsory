@model List<Frontend.Models.BannerViewModel>

@if (Model.Count() > 0)
{
    <div class="swiper-container sw-slideshow">
        <div class="swiper-wrapper">
            @foreach (var item in Model)
            {
                var target = "";
                if (item.OpenInNewTab == true)
                {
                    target = "_blank";
                }
                <div class="swiper-slide">
                    <img src="@item.ImageUrl" alt="" title="" class="img-fluid">
                </div>
            }
        </div>
    </div>
}



